<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="sunxikai928.com.github.springcloud.dao.IQuestionnireDAO">

    <!--查询问卷列表-->
    <select id="queryQuestionnire" resultType="QuestionnireInfo">
        SELECT
        t.ID id,
        t.TITLE title
        FROM QUESTIONNAIRE t
        WHERE t.ACTIVE = 'Y'
    </select>

</mapper>
